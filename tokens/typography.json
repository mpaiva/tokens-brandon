{
  "fontFamilies": {
    "heading": {
      "$type": "fontFamily",
      "$value": "Atkinson Hyperlegible Next"
    },
    "body": {
      "$type": "fontFamily",
      "$value": "Public Sans"
    },
    "label": {
      "$type": "fontFamily",
      "$value": "Public Sans"
    },
    "monospace": {
      "$type": "fontFamily",
      "$value": "Atkinson Hyperlegible Mono"
    }
  },
  "fontWeights": {
    "regular": {
      "$type": "fontWeight",
      "$value": "400"
    },
    "medium": {
      "$type": "fontWeight",
      "$value": "500"
    },
    "bold": {
      "$type": "fontWeight",
      "$value": "700"
    }
  },
  "lineHeights": {
    "xs": {
      "$type": "number",
      "$value": "1.4"
    },
    "sm": {
      "$type": "number",
      "$value": "1.45"
    },
    "md": {
      "$type": "number",
      "$value": "1.55"
